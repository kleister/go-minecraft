  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>kleister/.github//renovate/preset"
  ],
  "packageRules": [
    {
      "matchPackageNames": [
        "/^github.com\/golangci\/golangci-lint\/v2/"
      ],
      "labels": [
        "tools",
        "golangci"
      ]
    },
    {
      "matchPackageNames": [
        "/^github.com\/mgechev\/revive/"
      ],
      "labels": [
